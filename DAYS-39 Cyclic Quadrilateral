import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
        // Create scanner object to read inputs
        Scanner sc = new Scanner(System.in);

        // Read the number of test cases
        int T = sc.nextInt();

        // Process each test case
        for (int t = 0; t < T; t++) {
            // Read four space-separated integers for the angles
            int A = sc.nextInt();
            int B = sc.nextInt();
            int C = sc.nextInt();
            int D = sc.nextInt();

            // Check the condition for a cyclic quadrilateral
            if (A + C == 180 && B + D == 180) {
                System.out.println("YES");
            } else {
                System.out.println("NO");
            }
        }

        // Close the scanner object to prevent memory leak
        sc.close();
    }
}
